<?xml encoding="utf-8"?>
<!-- Statute DTD v2.3.1 == 20050311 -->

<!-- Statute DTD v2.3.0 == 20050104 -->

<!-- Statute DTD v2.2.0 == 20040916: No modifications -->

<!-- Statute DTD v2.1.0 == 20040715 -->

<!-- Statute DTD v2.0.0 == 20040528 -->

<!-- Sep. 2002 - removed the "inline" attribute from Provision since
	it was redundant.  -->

<!-- Feb. 2002 - removed common elements to a separate file called
     billCommonElements.ent...Julie Julian  -->
             
<!-- The Statute DTD ignores the change attribute if it is the only attribute 
     for an element.  If the element has other attributes (besides change) 
     then we can include the ATTLIST in  the DTD, but we map the %changeatt; 
     entity to the null string so that the changeatt is not included. 
<!ENTITY % ignorechange 'IGNORE'>
 -->
<!-- The Statute DTD includes the statute entity which allows the HistoricalNote
element while excluding the CommentBlock element.
<!ENTITY % statute 'INCLUDE'>
<!ENTITY % exchange 'IGNORE'>
<!ENTITY % authoring 'IGNORE'>
 -->
<!-- The Statute DTD includes the flat entity which defines the flattened model for the Body and Heading elements.
<!ENTITY % flat 'INCLUDE' >
<!ENTITY % nested 'IGNORE' >
 -->

<!ENTITY % changeatt "">

<!-- The following line identifies the billCommonElements.dtd to be included 
     in Statute.dtd -->

<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                             ENTITIES                                -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->


<!ENTITY % lowleveltext  "#PCDATA | Sub | Sup | Emphasis | Language 
                           | FootnoteRef | XRefInternal | XRefExternal
                           | PageBreak | Keep | LineBreak | HistoricalNoteSubItem" >

<!ENTITY % inline  "(#PCDATA | Sub | Sup | Emphasis 
                     | DefinedTermEn | DefinedTermFr | DefinitionEnOnly | DefinitionFrOnly | DefinitionRef | FootnoteRef 
                     | Language | LeaderRightJustified | Leader | XRefInternal 
                     | XRefExternal | HistoricalNote | Repealed 
					 | PageBreak | Keep | ImageGroup | Fraction | FormBlank)*" >


<!ENTITY % tbl.entry.mdl  "(#PCDATA | Sub | Sup | Emphasis | BilingualGroup 
               | FootnoteRef | Formula | XRefInternal 
               | XRefExternal | ImageGroup | Provision
               | Language | Leader | SignatureBlock | LeaderRightJustified 
               | PageBreak | Keep)* " >

<!ENTITY % blockfragment  "BilingualGroup
                         | FormulaGroup
                         | FormulaDefinition 
                         | FormulaParagraph
                         | FormGroup 
                         | TableGroup
                         | SignatureBlock
                         | Footnote 
                         | List
                         | Note
                         | Oath
                         | ImageGroup
                         | ReadAsText" >

                           
<!ENTITY % blocktext  "( %blockfragment; | Provision)" >
    

<!ENTITY % formula-block  "(FormulaGroup | ImageGroup | TableGroup | Provision | List)" >

<!ENTITY % provisionblock "(
                               BilingualGroup
                             | FormulaGroup
                             | FormGroup
                             | TableGroup
                             | SignatureBlock
                             | Footnote
                             | Note
							 | List
                             | Oath
                             | ImageGroup
                           )" >
       
	   

<!ENTITY % schedulecontent  "(BillPiece | DocumentInternal | BillInternal
                            | %blocktext; | Schedule | HistoricalNote | Repealed)*" >



<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                        HIGHEST LEVEL ELEMENT                        -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->

<!ELEMENT Statute  (Identification, Repealed?, Introduction?, Body?, Schedule*, RelatedProvisions?, NotInForceProvisions?) >
<!ATTLIST Statute
               bill-origin (commons | senate)   #REQUIRED
               bill-type (private-public | private-private | govt-public) 
               						"govt-public"
               document-id  CDATA    #IMPLIED 
               date-time  CDATA    #IMPLIED 
               startdate  CDATA    #IMPLIED  
               enddate  CDATA    #IMPLIED 
               reverseTitle  CDATA    #IMPLIED 
               alpha  CDATA    #IMPLIED 
               hasPreviousVersion  CDATA    #IMPLIED 
               xml:lang (en | fr)   #REQUIRED  
		in-force (yes | no) "yes" >
		
		
		
	<!ELEMENT AmendedContent ((Group | Provision | GroupHeading)+, HistoricalNote?) >


	<!ELEMENT Clause  (Label, Text, ((Subclause | %blocktext;)+, 
                   ContinuedClause?)*, HistoricalNote?) >                 
	<!ATTLIST Clause
               id  ID    #IMPLIED 
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT DocumentInternal
    		( (Group | Provision | GroupHeading)+, Schedule*, HistoricalNote? ) >
   

	<!ELEMENT Enacts  (Provision, HistoricalNote?) >


	<!ELEMENT Group 
  	 	(GroupHeading,
        	(Group? | Provision?)*,
	         HistoricalNote?
    		)>   
	<!ATTLIST Group
    	id  ID    #IMPLIED >



	<!ELEMENT Label  (%lowleveltext;)* >

	<!ELEMENT MarginalNote  (%lowleveltext;
                          | DefinedTermEn | DefinedTermFr
                          | DefinitionEnOnly | DefinitionFrOnly | DefinitionRef
								  | HistoricalNote
                         )* >
	<!ATTLIST MarginalNote id  ID    #IMPLIED  >

<!ELEMENT Item (Label?, Text, List?) >

	<!ELEMENT List (Item+) >
	<!ATTLIST List
		style (arabic | roman-lc | roman-uc | alphabet-lc | alphabet-uc | bullet | en-dash | em-dash) #IMPLIED 
		subsequent-line-indent CDATA #IMPLIED 
		first-line-indent CDATA #IMPLIED 
		justification (justify | left | right | center) #IMPLIED
		hyphenation (yes | no) #IMPLIED
		pointsize (6 | 7 | 8 | 9 | 10 | 11 | 12) #IMPLIED
		keep-with-next (yes | no) #IMPLIED
		keep-with-previous (yes | no) #IMPLIED
		topmarginspacing CDATA #IMPLIED
		xml:lang (en | fr)   #IMPLIED 
		language-align (yes | no) "no"
		%changeatt;  >
		
	<!ELEMENT Paragraph  (MarginalNote?, Label, Text, 
                     (((Subparagraph | %blocktext;)+, ContinuedParagraph?)* 
                     | AmendedText?), HistoricalNote?) >                
	<!ATTLIST Paragraph
               id  ID    #IMPLIED 
               type (normal | transitional | amending | CIF | application)   #IMPLIED  >


	<!ELEMENT Preamble  (Provision+, HistoricalNote?) >
	<!ATTLIST Preamble
               salutation   (yes | no ) "no"   >


	<!ELEMENT Provision (
                      MarginalNote?,
                      Label?,
                      ProvisionHeading?,
                      (
                        (
                          Text,
                          (
                            (
                              (Provision | %provisionblock;)+,
                              Text?
                            )*
                           |
                            AmendedContent?
                           |
                            ReadAsContent?
                          ),
                          HistoricalNote?
                        )
                       |
                        (Provision+, HistoricalNote?)
                       |
                        (%provisionblock;, HistoricalNote?)
                      )
                    ) >
	<!ATTLIST Provision	
                format-ref  CDATA    #IMPLIED
                first-line-indent CDATA #IMPLIED
                subsequent-line-indent CDATA #IMPLIED
                justification (justify | left | right | center) #IMPLIED
                hyphenation (yes | no) #IMPLIED
                pointsize (6 | 7 | 8 | 9 | 10 | 11 | 12) #IMPLIED
                keep-with-next (yes | no) #IMPLIED
                keep-with-previous (yes | no) #IMPLIED
                topmarginspacing CDATA #IMPLIED
		list-item (yes | no) "no"
                xml:lang (en | fr)   #IMPLIED 
                id  ID    #IMPLIED
                language-align (yes | no) "no"
                %changeatt; >


	<!ELEMENT Section  (MarginalNote*, Label, 
                    ((Text*, (Definition* | (((Paragraph | %blocktext;)+, 
                    ContinuedSectionSubsection?)* | (AmendedText, ContinuedSectionSubsection?)?))) | Subsection+),
                    HistoricalNote?) >        
	<!ATTLIST Section
               id  ID    #IMPLIED 
               target  CDATA    #IMPLIED 
               dateValidite  CDATA    #IMPLIED 
               iroId  CDATA    #IMPLIED 
               iroFid  CDATA    #IMPLIED 
               type (normal | transitional | amending | CIF | application)   #IMPLIED  >


	<!ELEMENT Subclause  (Label, Text, ((Subsubclause | %blocktext;)+,
                      ContinuedSubclause?)*, HistoricalNote?) >                    
	<!ATTLIST Subclause
               id  ID    #IMPLIED 
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT Subparagraph  (Label, Text, (((Clause 
                         | %blocktext;)+, ContinuedSubparagraph?)* 
                         | AmendedText?), HistoricalNote?) >                       
	<!ATTLIST Subparagraph
               id  ID    #IMPLIED 
               type (normal | transitional | amending)   #IMPLIED  >


	<!ELEMENT Subsection  (MarginalNote*, Label, Text*, 
                       (Definition* | (((Paragraph | %blocktext;)+,
                       ContinuedSectionSubsection?)* | (AmendedText, ContinuedSectionSubsection?)?)),
                       HistoricalNote?) >                      
	<!ATTLIST Subsection
               id  ID    #IMPLIED 
               type (normal | transitional | amending | CIF | application)   #IMPLIED  >


	<!ELEMENT Subsubclause  (Label, Text, 
                         (%blocktext;)*, HistoricalNote?) >
	<!ATTLIST Subsubclause
               id  ID    #IMPLIED 
               type  (normal | transitional | amending)  #IMPLIED  >



<!--  These elements' model is part of the entity as their model 
includes the CommonBlock element in the Authoring DTD.-->

	<!ENTITY % docpart  "(LongTitle | Preamble | (Heading | Section)+ 
                     | Subsection+ | SectionPiece | (ScheduleFormHeading, %schedulecontent;)
                     | (%blockfragment;)+ | Formula | (Group | GroupHeading | Provision)+)" >


	<!ELEMENT ContinuedClause  (Text) >
	<!ATTLIST ContinuedClause
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT ContinuedDefinition  (Text) >

	<!ELEMENT ContinuedParagraph  (Text) >
	<!ATTLIST ContinuedParagraph
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional | amending)  #IMPLIED  >



	<!ELEMENT ContinuedSectionSubsection  (Text, (AmendedText | ReadAsText)?) >
	<!ATTLIST ContinuedSectionSubsection
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT ContinuedSubclause  (Text) >
	<!ATTLIST ContinuedSubclause
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT ContinuedSubparagraph  (Text) >
	<!ATTLIST ContinuedSubparagraph
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional | amending)  #IMPLIED  >


	<!ELEMENT Definition  ( 
                        MarginalNote*,
                        (Text*,((Paragraph | %blocktext;)+,ContinuedDefinition?)*)
                      )>


	<!ELEMENT SectionPiece  (
                           Definition
                         | Paragraph 
                         | Subparagraph 
                         | Clause 
                         | Subclause 
                         | Subsubclause 
                         | ContinuedSectionSubsection 
                         | ContinuedParagraph 
                         | ContinuedSubparagraph 
                         | ContinuedClause 
                         | ContinuedSubclause 
                         | ContinuedDefinition
                         | %blocktext; 
                        )+ >

	<!ELEMENT Summary  (TitleText, Provision+) >







	<!ELEMENT Body  (Heading | Section )+ >



	<!ELEMENT Heading  (Label?, MarginalNote?, TitleText?, Note?) >
	<!ATTLIST Heading
	    level (1 | 2 | 3 | 4 | 5) #REQUIRED
	    style CDATA  #IMPLIED 
	    type (normal | transitional | amending) #IMPLIED
	    %changeatt;  >

<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                       IDENTIFICATION ELEMENT                        -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->
   
<!ELEMENT Identification  (BillNumber?, Parliament?, LongTitle, 
                           ShortTitle, RunningHead?, BillHistory, BillSponsor?, StatuteYear?, 
                           Chapter?, ReaderNote?, BillRefNumber?, PreviousTitle?, NextTitle?) >

                           
<!ELEMENT Chapter  (ConsolidatedNumber?, AnnualStatuteId?) >


<!ELEMENT AnnualStatuteId  (AnnualStatuteNumber, YYYY ) >
<!ATTLIST AnnualStatuteId
               revised-statute (yes | no) "no"  >


<!ELEMENT ConsolidatedNumber (#PCDATA) >
<!ATTLIST ConsolidatedNumber official ( yes | no ) #REQUIRED >


<!ELEMENT ReaderNote  (Note+) >

<!ELEMENT HistoricalNote  (%lowleveltext;)* >  

<!ELEMENT HistoricalNoteSubItem  (%lowleveltext;)*  >

<!ELEMENT StatuteYear  (#PCDATA) >

<!ELEMENT PreviousTitle (Chapter, LongTitle, ShortTitle)+>

<!ELEMENT NextTitle (Chapter, LongTitle, ShortTitle)+>
<!ELEMENT Parliament  (Session, Number, RegnalYear, Year-s) >


<!ELEMENT LongTitle  (%lowleveltext;)* >

<!ATTLIST LongTitle
               type CDATA   #IMPLIED  >


<!ELEMENT ShortTitle  (%lowleveltext;)* >

<!ATTLIST ShortTitle
               status (official | unofficial)   #REQUIRED  >


<!ELEMENT RunningHead (%lowleveltext;)* >

               
<!ELEMENT AnnualStatuteNumber   (%lowleveltext;)* >


<!ELEMENT BillSponsor  (#PCDATA) >
<!ATTLIST BillSponsor
               billsponsorid  CDATA    #IMPLIED  >
                             

<!ELEMENT BillRefNumber  (#PCDATA) >
<!ATTLIST BillRefNumber
               date-time  CDATA    #IMPLIED  >


<!ELEMENT BillHistory  (Stages+, RelatedInformation*) >


<!ELEMENT Session  (#PCDATA) >


<!ELEMENT Number  (#PCDATA) >


<!ELEMENT RegnalYear  (Year-s, Monarch) >


<!ELEMENT Year-s  (#PCDATA) >


<!ELEMENT Monarch  (#PCDATA) >


<!ELEMENT Stages  (Date?, StageText?) >
<!ATTLIST Stages
               stage (tabled | first-reading-house | second-reading-house 
               | committee-house | report-house | third-reading-house 
               | first-reading-senate | second-reading-senate | committee-senate 
               | report-senate | third-reading-senate | assented-to
               | gazette | consolidation)   #REQUIRED  >
              

<!ELEMENT StageText  (%lowleveltext;)* >


<!ELEMENT RelatedInformation  (%lowleveltext;)* >


<!ELEMENT Date  (YYYY, MM, DD) >


<!ELEMENT YYYY  (#PCDATA) >


<!ELEMENT MM  (#PCDATA) >


<!ELEMENT DD  (#PCDATA) >


<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                         INTRODUCTION ELEMENT                        -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->

<!ELEMENT Introduction  (Recommendation?, Summary?, TableOfProvisions?, 
((Preamble, Enacts?) | Enacts)?, HistoricalNote?) >

<!ELEMENT Recommendation  (TitleText, Provision) >


<!ELEMENT TableOfProvisions (Heading | (Label?, Text))+  >
<!ATTLIST TableOfProvisions
               label-column-width (2.5 | 3 | 3.5 | 4 | 4.5)   "2.5"  >


<!ELEMENT TitleText  (%lowleveltext;)* >

                

<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                       BLOCK TEXT ELEMENTS                           -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->
                                                                                      
<!ELEMENT AmendedText  (%docpart; | BillInternal | OriginatingRef | Schedule) >
<!ATTLIST AmendedText
               xml:lang (en | fr )   #IMPLIED 
               change  (ins | off) #IMPLIED
               include-in-TableOfProvisions (yes | no)  "no" >


<!ELEMENT BillNumber  (#PCDATA) >


<!ELEMENT BillPiece  (Preamble | Body | (Heading | Section)+ ) >


<!ELEMENT BillInternal  (LongTitle, Preamble?, Enacts?, Body, Schedule*) >


<!ELEMENT BilingualGroup  (TitleText?, (BilingualItem* | (BilingualItemEn, BilingualItemFr)+ | 
					(BilingualItemFr, BilingualItemEn)+)) >

<!ELEMENT BilingualItem  ((BilingualItemEn, BilingualItemFr)+ | (BilingualItemFr, BilingualItemEn)+) >

<!ELEMENT BilingualItemEn  (%lowleveltext;)* >
<!ATTLIST BilingualItemEn  font-style (regular | italic) #IMPLIED >


<!ELEMENT BilingualItemFr  (%lowleveltext;)* >
<!ATTLIST BilingualItemFr font-style (regular | italic) #IMPLIED  >


<!ELEMENT ImageGroup  (title*, AlternateText?, Caption*, Image) >
<!ATTLIST ImageGroup
               id  ID    #IMPLIED 
               %changeatt;
               bilingual (yes | no) "no"
               spanlanguages	 (yes | no) "no"
               spanmarginalnotecol	(yes | no) "no"
		position (left | right | center | inline)  "center"
		width  CDATA    #IMPLIED 
		float (right | left | top | bottom) #IMPLIED  
     >

<!ELEMENT Fraction (Numerator, Denominator) >
<!ATTLIST Fraction 
		style (oblique | horizontal) #IMPLIED >
<!ELEMENT Numerator (#PCDATA | MSup | MSub)* >
<!ELEMENT Denominator (#PCDATA | MSup | MSub)* >
<!ELEMENT MSup (Base, Superscript) >
<!ELEMENT MSub (Base, Subscript) >
<!ELEMENT Base (#PCDATA) >
<!ELEMENT Superscript (#PCDATA) >
<!ELEMENT Subscript (#PCDATA) >
					 

<!ELEMENT AlternateText (#PCDATA)>


<!ELEMENT Image   EMPTY  >
<!ATTLIST Image
               source  CDATA    #IMPLIED  >


<!ELEMENT Caption  (%lowleveltext;)* >
<!ATTLIST Caption
               position (over | under )  "under"
					justification (justify | left | right | center) #IMPLIED
					change  (ins | del | off | alt)   #IMPLIED 
					pointsize (6 | 7 | 8 | 9 | 10 | 11 | 12) #IMPLIED 
               >

<!ATTLIST Definition
               id  ID    #IMPLIED 
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >
                         
<!ELEMENT DefinedTermEn  (%lowleveltext;)* >

<!ATTLIST DefinedTermEn
               style  (bold | italic | normal)    #IMPLIED  >
               
<!ELEMENT DefinedTermFr  (%lowleveltext;)* >

<!ATTLIST DefinedTermFr
               style  (bold | italic | normal)    #IMPLIED  >
               
<!ELEMENT DefinitionEnOnly  (%lowleveltext;)* >


<!ELEMENT DefinitionFrOnly  (%lowleveltext;)* >

<!ELEMENT DefinitionRef  (%lowleveltext;)* >
               
<!ELEMENT Footnote  (Label, Text) >
<!ATTLIST Footnote
               id  ID    #IMPLIED 
               placement (table | section)   #IMPLIED 
		status (official | unofficial | editorial) "official"
		xml:space (default | preserve) "default"
               %changeatt;  >

<!ELEMENT FormBlank %inline; >
<!ATTLIST FormBlank
	width CDATA #REQUIRED >
	
	
<!ELEMENT Formula  (FormulaText | ImageGroup)+ >
<!ATTLIST Formula
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >


<!ELEMENT FormulaText  (%lowleveltext; | Fraction)* >


<!ELEMENT FormulaTerm  %inline; >


<!ELEMENT FormulaConnector  (#PCDATA) >
<!ATTLIST FormulaConnector
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >


<!ELEMENT FormulaDefinition  (FormulaTerm, (Text, ((FormulaParagraph | %formula-block;)+, 
                       ContinuedFormulaParagraph?)*), ContinuedFormulaDefinition?) >                    
<!ATTLIST FormulaDefinition
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >


<!ELEMENT FormulaParagraph  (Label, Text, ((FormulaParagraph | %formula-block;)+, 
                        ContinuedFormulaParagraph?)*) >                       
<!ATTLIST FormulaParagraph
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;
               type  (normal | transitional )  #IMPLIED >


<!ELEMENT FormulaGroup  (AlternateText?, Formula, FormulaConnector?, (FormulaDefinition* 
                         | FormulaParagraph*)) >
<!ATTLIST FormulaGroup
               id  ID    #IMPLIED 
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;  >


<!ELEMENT FormGroup  (AlternateText?, ScheduleFormHeading?, (SignatureBlock
                      | Provision | List | TableGroup)+) >                     
<!ATTLIST FormGroup
               id  ID    #IMPLIED 
               %changeatt;  >


<!ELEMENT ContinuedFormulaParagraph  (Text) >
<!ATTLIST ContinuedFormulaParagraph
               indent-level (0 | 1 | 1.5 | 2 | 2.5 | 3 | 3.5 | 4 | 4.5 | 5 | 5.5 | 6 | 6.5)   #IMPLIED
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >


<!ELEMENT ContinuedFormulaDefinition  (Text) >
<!ATTLIST ContinuedFormulaDefinition
               %changeatt;
               type  (normal | transitional )  #IMPLIED  >



<!ELEMENT Note  (%lowleveltext; | Provision)* >
<!ATTLIST Note
               id  ID    #IMPLIED 
		status (official | unofficial | editorial) "official"
		xml:space (default | preserve) "default"
	        %changeatt;  >


<!ELEMENT Oath  (%lowleveltext; | Leader)* >


<!ELEMENT ReadAsText (Section* | Subsection* | SectionPiece | FormulaGroup) >


<!ELEMENT SignatureBlock  (SignatureLine | SignatureTitle | SignatureName)+ >

  
<!ELEMENT SignatureLine  (%lowleveltext; | Leader)* >

<!ELEMENT SignatureTitle  (%lowleveltext;)* >

<!ELEMENT SignatureName  (%lowleveltext;)* >

<!ELEMENT TableGroup  (AlternateText?, Caption?, table, Footnote* ) >
<!ATTLIST TableGroup
               id  ID    #IMPLIED 
               %changeatt;  
			pointsize NMTOKEN #IMPLIED
               rowbreak  (yes | no) "no"
               bilingual (yes | no) "no"
               isRow (yes | no) "no"
               spanlanguages	 (yes | no) "no"
			spanmarginalnotecol (yes | no) "no"  
			orientation (landscape | portrait) #IMPLIED 
			topmarginspacing CDATA #IMPLIED
			bottommarginspacing CDATA #IMPLIED  
                 >



<!ELEMENT Text  %inline; >
<!ATTLIST Text
               id  ID    #IMPLIED  >

                                         
<!-- ____________________________________________________________________-->
<!--                                                                     -->
<!--                       INLINE TEXT ELEMENTS                          -->
<!--                                                                     -->
<!-- ____________________________________________________________________-->

<!ELEMENT Repealed  (%lowleveltext;)* >

<!ELEMENT Emphasis  (%lowleveltext;)* >
<!ATTLIST Emphasis
               style (bold | italic | underline | smallcaps | overbar | regular) #REQUIRED  >


<!ELEMENT FootnoteRef  (%lowleveltext;)* >
<!ATTLIST FootnoteRef
               idref  IDREFS    #REQUIRED >


<!ELEMENT Language  (%lowleveltext;)* >
<!ATTLIST Language
               xml:lang (en | fr | la )   #REQUIRED  >


<!ELEMENT LeaderRightJustified  EMPTY >
<!ATTLIST LeaderRightJustified
